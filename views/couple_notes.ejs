		<div>
			<h1>Notes:</h1>
			<div class="container">
			  <div class="row">
			    <div class="col-xs-6">
					<% myNotes.forEach(function(nt) { %>
						<div class="box blue">
							<a href="/users/<%=user.id%>/notes/<%=nt.id%>"><%= nt.title %></a>
							<%= nt.UserId %>
							<%= nt.user_id %>
						</div>
					<% }) %>
				</div>
			    <div class="col-xs-6">
					<% pNotes.forEach(function(nt) { %>
						<div class="box pink">
							<a href="/users/<%=user.id%>/notes/<%=nt.id%>"><%= nt.title %></a>
							<%= nt.UserId %>
							<%= nt.user_id %>
						</div>
					<% }) %>
				</div>
			  </div>
			</div>
				<p id="demo" onclick="myFunction(this, 'red')">Click me to change my text color.</p>

<script>
function myFunction(elmnt,clr) {
    elmnt.style.color = clr;
}
</script>

			    <!-- Modal content-->
			    <div class="modal-content">
			      <div class="modal-header">
			        <button type="button" class="close" data-dismiss="modal">&times;</button>
			        <h4 class="modal-title">Note Title</h4>
			      </div>
			      <div class="modal-body">
			        <p>The text of the note will be shown here.</p>
			      </div>
			      <div class="modal-footer">
			        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
			      </div>
